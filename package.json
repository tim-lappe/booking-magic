{
  "name": "tl-booking",
  "description": "flexible and powerful Booking-Plugin for WordPress",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "dev:start": "npm run env:start; npm run watch",
    "env:start": "docker-compose up -d wordpress wordpress_phpmyadmin",
    "env:stop": "docker-compose down",
    "watch": "cd assets/js; webpack --watch",
    "test": "docker-compose run -w /var/www/html/wp-content/plugins/booking-magic wordpress_it php ./vendor/bin/phpunit"
  },
  "devDependencies": {
    "ftp-deploy": "^2.4.1",
    "ts-loader": "^8.0.17",
    "typescript": "^4.2.3",
    "webpack": "^5.24.3",
    "webpack-cli": "^4.5.0"
  }
}
