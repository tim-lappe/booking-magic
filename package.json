{
  "name": "tl-booking",
  "description": "flexible and powerful Booking-Plugin for WordPress",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "build": "webpack build",
    "dev:start": "npm run env:start; npm run watch",
    "env:start": "docker-compose up -d wordpress wordpress_phpmyadmin",
    "env:stop": "docker-compose down --remove-orphans",
    "watch": "webpack --watch",
    "test": "docker-compose run -w /var/www/html/wp-content/plugins/booking-magic wordpress_it php ./vendor/bin/phpunit"
  },
  "devDependencies": {
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "object-assign": "^4.1.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-loader": "^9.2.6",
    "tslint": "^6.1.3",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "inversify": "^6.0.1",
    "reflect-metadata": "^0.1.13"
  }
}
